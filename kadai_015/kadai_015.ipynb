    "class Human:\n",
    "    def __init__(self, name, age):\n",
    "        self.name = name\n",
    "        self.age = age\n",
    "    \n",
    "    def printinfo(self):\n",
    "        print(f\"名前：{self.name} 年齢：{self.age}\")\n",
    "\n",
    "# インスタンス化\n",
    "personal = Human(\"山田太郎\", 30)\n",
    "personal.printinfo()\n"
   ]
